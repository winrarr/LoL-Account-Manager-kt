<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="170.0" minWidth="-Infinity" prefHeight="170.0"
      prefWidth="243.0" spacing="5.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <HBox alignment="CENTER_LEFT" prefHeight="124.0" prefWidth="262.0" spacing="10.0">
        <ComboBox fx:id="serverCmb" maxWidth="60.0" minWidth="60.0" prefHeight="25.0" prefWidth="60.0"/>
        <Label maxWidth="40.0" minWidth="40.0" prefHeight="17.0" prefWidth="40.0" text="Player:"/>
        <TextField fx:id="playerTxt" prefHeight="25.0" prefWidth="123.0" HBox.hgrow="ALWAYS"/>
    </HBox>
    <GridPane>
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="70.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="300.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <Label text="IGN:"/>
        <Label text="Username:" GridPane.rowIndex="1"/>
        <Label text="Password:" GridPane.rowIndex="2"/>
        <TextField fx:id="nameTxt" GridPane.columnIndex="1"/>
        <TextField fx:id="usernameTxt" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
        <TextField fx:id="passwordTxt" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
    </GridPane>
    <Button fx:id="addAccountBtn" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#addAccount"
            text="Add Account"/>
</VBox>
