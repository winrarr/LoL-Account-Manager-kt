          <RowConstraints minHeight="35.0" prefHeight="35.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label prefHeight="44.0" prefWidth="40.0" text="Search:">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </Label>
            <TextField fx:id="searchTxt" prefHeight="25.0" prefWidth="257.0" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" />
