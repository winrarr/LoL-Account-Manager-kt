            <Button fx:id="removeAccountBtn" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#removeSelectedAccount" text="Remove Account" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <GridPane alignment="CENTER_LEFT" hgap="10.0" VBox.vgrow="NEVER">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="76.0" minWidth="76.0" prefWidth="76.0" />
